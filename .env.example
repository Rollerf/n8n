# Copy to .env and customize before running docker compose
# Public host/protocol should reflect the URL users hit (e.g. https://n8n.example.com)
N8N_HOST=n8n.example.com
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_API_BASE_URL=https://n8n.example.com/rest
N8N_PUBLIC_API_BASE_URL=https://n8n.example.com
N8N_EDITOR_BASE_URL=https://n8n.example.com
# Used by n8n to build links for live/test webhooks; set to your external base URL
N8N_WEBHOOK_URL=https://n8n.example.com
N8N_WEBHOOK_TUNNEL_URL=https://n8n.example.com
WEBHOOK_URL=https://n8n.example.com
WEBHOOK_TUNNEL_URL=https://n8n.example.com
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=changeme
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme
N8N_ENCRYPTION_KEY=replace-with-a-32-character-hex-key
HOST_UID=1000
HOST_GID=1000
